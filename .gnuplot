set term x11
set grid

# set position of the title 
set key left above
set key Left vertical
set key reverse

# draw x-axis (y=0)
set xzeroaxis

set linetype  1  lt  1  lc rgbcolor "red"            pt 1
set linetype  2  lt  2  lc rgbcolor "black"     pt 2
set linetype  3  lt  3  lc rgbcolor "blue"           pt 3
#set linetype  4  lt  4  lc rgbcolor "magenta"        pt 4
set linetype  5  lt  5  lc rgbcolor "dark-cyan"      pt 5
set linetype  6  lt  6  lc rgbcolor "brown"          pt 6
set linetype  7  lt  7  lc rgbcolor "navy"           pt 7
set linetype  8  lt  8  lc rgbcolor "orange-red"     pt 8
set linetype  9  lt  9  lc rgbcolor "purple"         pt 9
set linetype 10  lt 10  lc rgbcolor "gray20"         pt 10
set linetype 11  lt 11  lc rgbcolor "dark-red"       pt 11
set linetype 12  lt 12  lc rgbcolor "sea-green"      pt 12
set linetype 13  lt 13  lc rgbcolor "dark-blue"      pt 13
set linetype 14  lt 14  lc rgbcolor "dark-magenta"   pt 14
set linetype 15  lt 15  lc rgbcolor "dark-violet"    pt 15
set linetype 16  lt 16  lc rgbcolor "brown"          pt 16
set linetype 17  lt 17  lc rgbcolor "dark-goldenrod" pt 17 
set linetype 18  lt 18  lc rgbcolor "dark-salmon"    pt 18
set linetype 19  lt 19  lc rgbcolor "dark-khaki"     pt 19
set linetype 20  lt 20  lc rgbcolor  "gray40"        pt 20
